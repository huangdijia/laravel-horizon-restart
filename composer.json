{
    "name": "huangdijia/laravel-horizon-restart",
    "description": "horizon-restart for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "migrate",
        "recreate"
    ],
    "homepage": "https://github.com/huangdijia/laravel-horizon-restart",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/console": "^9.0|^10.0",
        "illuminate/bus": "^9.0|^10.0",
        "laravel/horizon": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^7.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Huangdijia\\Horizon\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "2.x-dev": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Huangdijia\\Horizon\\RestartServiceProvider"
            ]
        }
    },
    "scripts": {
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src",
        "cs-fix": "php-cs-fixer fix $1"
    }
}
